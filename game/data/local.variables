# Used only at window creation.
:/display
    debug_context_version   4.4
    release_context_version 3.0

    vsync true
    frame_cap 120
    fullscreen false
    window_size 1280 720


:/gameplay
    # Physic updates per second
    ups 60

# Probably shut be cut of from public game builds?
:/debug
    dev_console false
    gl_debug    true
